{
  "name": "mobx",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack-dev-server --d ./src/main.js --output ./stage/main.js --open",
    "prod": "webpack --p ./src/main.js --output ./build/main.js",
    "server": "json-server --watch db.json --port 7777",
    "start-prod": "npm run prod && cross-env NODE_ENV=production && forever server.js && json-server --watch db.json --port 7777",
    "start": "node server.js",
    "heroku-postbuild": "webpack -p"
  },
